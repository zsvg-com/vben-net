{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "SqlSugar": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "【时间】{Timestamp:yyyy-MM-dd HH:mm:ss,fff}{NewLine}【等级】{Level:u3}{NewLine}【消息】{Message:lj}{NewLine}{NewLine}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "../../zsvg/logs/.log",
          "rollingInterval": "Day",
          "outputTemplate": "【时间】{Timestamp:yyyy-MM-dd HH:mm:ss,fff}{NewLine}【等级】{Level:u3}{NewLine}【消息】{Message:lj}{NewLine}{NewLine}"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:8080"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "AllowedHosts": "*",
  "CorsAccessorSettings": {
    "WithExposedHeaders": [
      "download-filename"
    ],
    "PolicyName": "自定义跨域策略名",
    "WithOrigins": [
      "http://xxx.com:868",
      "http://localhost:868"
    ]
  },
  "DynamicApiControllerSettings": {
    "CamelCaseSeparator": "/",
    "DefaultRoutePrefix": "",
    "AbandonControllerAffixes": [
      "Controller",
      "Api"
    ]
  },
  "SpecificationDocumentSettings": {
    "EnableAuthorized": true,
    "DocExpansionState": "None",
    "DocumentTitle": "Api接口清单",
    "DefaultGroupName": "Default",
    "GroupOpenApiInfos": [
      {
        "Group": "Default",
        "Order": 100,
        "Title": "自建应用",
        "Description": "这里是自定义的业务模块，在测试API请求之前，请先通过auth/login接口登录",
        "Version": "V1.0.0",
        "Contact": {
          "Name": "前台UI访问地址",
          "Url": "http://localhost:5666/"
        }
      },
      {
        "Group": "Auth",
        "Order": 9,
        "Title": "认证授权",
        "Description": "登录，注册，注销，鉴权等",
        "Version": "V1.0.0"
      },
      {
        "Group": "Sys",
        "Order": 8,
        "Title": "系统管理",
        "Description": "系统管理主要包括组织架构，权限管理，一般只有系统管理员有此分组的权限",
        "Version": "V1.0.0"
      },
      {
        "Group": "Mon",
        "Order": 7,
        "Title": "监控中心",
        "Description": "监控中心主要包括系统日志，定时任务，服务监控等，一般只有系统管理员有此分组的权限",
        "Version": "V1.0.0"
      },
      {
        "Group": "Tool",
        "Order": 6,
        "Title": "辅助工具",
        "Description": "辅助模块主要有数据字典，代码生成器等，为业务模块服务",
        "Version": "V1.0.0"
      },
      {
        "Group": "Pub",
        "Order": 5,
        "Title": "公共模块",
        "Description": "公共模块一般只要登录了，不管什么角色，都应有的通用公共权限",
        "Version": "V1.0.0"
      },
      {
        "Group": "Bpm",
        "Order": 4,
        "Title": "流程引擎",
        "Description": "流程引擎是针对流程的专门模块",
        "Version": "V1.0.0"
      }
    ]
  }
}